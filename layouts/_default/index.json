[
  {{- range $i, $e := .Site.RegularPages -}}
  {{- if $i }},{{ end }}
    { "id": {{ $i | jsonify }},
      "uri": {{ .RelPermalink | jsonify}},
      "section": {{ .Parent.Title | jsonify }},
      "title": {{ .Title | jsonify }},
      "content": {{ replace .Plain "\n" " " | strings.TrimSuffix " " | jsonify }},
      "tags": {{ $tags := partial "get/keywords" . }}{{ if $tags }}{{ delimit $tags ", " | jsonify }}{{ else }}""{{ end }}
    }
  {{- end}}
]
